{
  "name": "aoc2022",
  "version": "1.0.0",
  "description": "Advent of Code 2022 - solutions",
  "type": "module",
  "scripts": {
    "start": "yarn setup && aocrunner day",
    "setup": "rm -rf src/template src/tsconfig.json && cp -r ../template src/template && cp -r ../tsconfig.json.template tsconfig.json",
    "build": "aocrunner build",
    "format": "prettier -w src",
    "update:readme": "aocrunner update:readme"
  },
  "keywords": [
    "aoc"
  ],
  "author": "Deyan Varchev",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "aocrunner": "^1.9.6",
    "prettier": "^2.8.0"
  },
  "dependencies": {
    "aoc-utils": "../aoc-utils"
  },
  "engines": {
    "node": ">=18.13.0"
  }
}
